package com.todolistapplication.backend.serviceImpl;

import org.springframework.beans.factory.annotation.Autowired;
import org.springframework.stereotype.Service;

import com.todolistapplication.backend.entity.User;
import com.todolistapplication.backend.repository.UserRepository;
import com.todolistapplication.backend.service.UserService;

@Service
public class UserServiceImpl implements UserService{
	@Autowired
	private UserRepository userRepo;

	@Override
	public void registerUser(User user) {
		// TODO Auto-generated method stub
		userRepo.save(user);
		
		
	}

}
